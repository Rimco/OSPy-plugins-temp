#: plugins/remote_notifications/templates/remote_notifications.html:3
#: plugins/remote_notifications/templates/remote_notifications.html:25
msgid "Remote Notifications"
msgstr "Vzdálená oznámení"

#: plugins/remote_notifications/templates/remote_notifications.html:26
msgid ""
"This plugin can send Notifications on the Remote Webserver. This plugin "
"requre SQL server and Webserver with PHP."
msgstr ""
"Toto rozšíření zasílá oznámení na vzdálený webový server. Toto rozšíření "
"vyžaduje na vzdálené straně SQL Server a Webserver s PHP."

#: plugins/remote_notifications/templates/remote_notifications.html:27
msgid "This plugin send data as get command to remote PHP web pages."
msgstr ""
"Toto rozšíření zasílá data, jako GET příkaz na vzdálenou PHP webovou "
"stránku."

#: plugins/remote_notifications/templates/remote_notifications.html:28
msgid ""
"This plugin requred correct settings this plugins: UPS Monitor, Tank Humi "
"Monitor."
msgstr ""
"Toto rozšíření vyžaduje správné nastavení těchto rozšíření: UPS monitor, "
"Tank Humi monitor."

#: plugins/remote_notifications/templates/remote_notifications.html:32
msgid "Use plugin"
msgstr "Používat toto rozšíření"

#: plugins/remote_notifications/templates/remote_notifications.html:38
msgid "Remote server address"
msgstr "Adresa vzdáleného serveru"

#: plugins/remote_notifications/templates/remote_notifications.html:40
msgid "ex: https://server.com/automatOSPy/"
msgstr "příklad: https://server.com/automatOSPy/"

#: plugins/remote_notifications/templates/remote_notifications.html:44
msgid "API for access"
msgstr "API klíč pro přístup"

#: plugins/remote_notifications/templates/remote_notifications.html:50
msgid "Status"
msgstr "Stav"

#: plugins/remote_notifications/templates/remote_notifications.html:59
msgid "Submit"
msgstr "Potvrdit"

#: plugins/remote_notifications/templates/remote_notifications.html:60
msgid "Send test data to server"
msgstr "Odeslat test na server"

#: plugins/remote_notifications/templates/remote_notifications.html:61
msgid "Cancel"
